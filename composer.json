    "keywords": ["single", "instance", "mutex", "controller"],
    "homepage": "https://github.com/njspok/yii2-mutex-behavior",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Khamov Oleg",
            "email": "njspok@gmail.com"
        }
    ],
    "require": {
